<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/themeColor">
    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:stickyHeader="@+id/buttonLayout">
        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/formOfCareer1"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/formOfCareer1_Textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="30dp"
                        android:text="경력사항1"
                        android:gravity="center"/>
                    <LinearLayout
                        android:id="@+id/formOfCareer1_layout"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/formOfCareer1_Textview">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer1_name_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="회사명"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer1_name_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer1_office_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당부서"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer1_office_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer1_enterYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="입사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer1_enterYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer1_resignYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="퇴사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer1_resignYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer1_task_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당업무"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer1_task_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/formOfCareer2"
                    android:layout_below="@+id/formOfCareer1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/formOfCareer2_Textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/profile_picture_loadButton"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="30dp"
                        android:text="경력사항2"
                        android:gravity="center"/>
                    <LinearLayout
                        android:id="@+id/formOfCareer2_layout"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/formOfCareer2_Textview">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer2_name_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="회사명"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer2_name_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer2_office_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당부서"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer2_office_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer2_enterYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="입사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer2_enterYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer2_resignYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="퇴사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer2_resignYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer2_task_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당업무"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer2_task_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>

                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/formOfCareer3"
                    android:layout_below="@+id/formOfCareer2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/formOfCareer3_Textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/profile_picture_loadButton"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="30dp"
                        android:text="경력사항3"
                        android:gravity="center"
                        />
                    <LinearLayout
                        android:id="@+id/formOfCareer3_layout"
                        android:orientation="vertical"
                        android:layout_below="@+id/formOfCareer3_Textview"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer3_name_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="회사명"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer3_name_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer3_office_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당부서"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer3_office_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer3_enterYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="입사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer3_enterYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer3_resignYM_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="퇴사년월"
                            app:suffixText="YYYY/MM"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer3_resignYM_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/formOfCareer3_task_Layout"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="담당업무"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/formOfCareer3_task_EditText"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/buttonLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/backBtn"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="13dp"
                        android:src="@drawable/ic_baseline_arrow_back_24"
                        android:background="@drawable/btn_back"/>
                    <android.widget.Button
                        android:id="@+id/cmpltBtn"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/radius"
                        android:layout_marginTop="13dp"
                        android:textColor="@color/white"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="18dp"
                        android:text="@string/complete"/>
                </RelativeLayout>
            </RelativeLayout>
        </FrameLayout>
    </com.amar.library.ui.StickyScrollView>
</RelativeLayout>
