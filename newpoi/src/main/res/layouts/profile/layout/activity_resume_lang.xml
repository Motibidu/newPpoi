<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/themeColor">

    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:stickyHeader="@+id/buttonLayout">
        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:id="@+id/lang1"
                    android:layout_marginTop="10dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/text_lang1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginEnd="30dp"
                        android:text="어학사항1"
                        android:gravity="center"/>
                    <LinearLayout
                        android:id="@+id/layout_lang1"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_lang1">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_lang1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="언어"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_lang1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_test1"
                            android:layout_below="@+id/TextInputLayout_test1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="시험"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_test1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_score1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="점수"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_score1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_institution1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="기관"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_institution1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/lang2"
                    android:layout_below="@+id/lang1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/text_lang2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginLeft="25dp"
                        android:layout_marginRight="30dp"
                        android:text="어학사항2"
                        android:gravity="center"/>
                    <LinearLayout
                        android:layout_below="@id/text_lang2"
                        android:id="@+id/university_layout0"
                        android:orientation="vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_lang2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="언어"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_lang2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_test2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="시험"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_test2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_score2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="점수"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_score2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_institution2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="기관"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_institution2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/lang3"
                    android:layout_below="@+id/lang2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border">
                    <TextView
                        android:id="@+id/text_lang3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="25dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginEnd="30dp"
                        android:text="어학사항3"
                        android:gravity="center"
                        />
                    <LinearLayout
                        android:id="@+id/layout_lang3"
                        android:orientation="vertical"
                        android:layout_below="@+id/text_lang3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_lang3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="언어"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_lang3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_test3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="시험"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_test3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_score3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="점수"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_score3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_institution3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="기관"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="1">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_institution3"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/buttonLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/imageBtn_back"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="13dp"
                        android:src="@drawable/ic_baseline_arrow_back_24"
                        android:background="@drawable/btn_back"/>
                    <android.widget.Button
                        android:id="@+id/btn_complete"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/radius"
                        android:layout_marginTop="13dp"
                        android:textColor="@color/white"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="18dp"
                        android:text="@string/complete"/>
                </RelativeLayout>
            </RelativeLayout>
        </FrameLayout>
    </com.amar.library.ui.StickyScrollView>
</RelativeLayout>
