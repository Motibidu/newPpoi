<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/profile_setting_entry"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageButton
            android:id="@+id/back_button"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginLeft="18dp"
            android:layout_marginTop="13dp"
            android:src="@drawable/ic_baseline_arrow_back_24"
            android:background="@drawable/btn_back"/>
        <android.widget.Button
            android:id="@+id/complete_profile_setting_button"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/radius"
            android:layout_marginTop="13dp"
            android:textColor="@color/white"
            android:layout_alignParentRight="true"
            android:layout_marginRight="18dp"
            android:text="@string/complete"/>

        <ImageView
            android:id="@+id/profile_picture"
            android:layout_width="130dp"
            android:layout_height="130dp"
            android:layout_marginTop="60dp"
            android:scaleType="fitXY"
            android:adjustViewBounds="true"
            android:src="@drawable/ic_baseline_person_pin_24"
            android:backgroundTint="@color/white"
            android:layout_centerHorizontal="true"
            />
        <android.widget.Button
            android:id="@+id/profile_picture_loadButton"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:background="@drawable/radius"
            android:text="@string/modify"
            android:textColor="@color/white"
            android:layout_below="@+id/profile_picture" />
        <TextView
            android:id="@+id/eduBack"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/profile_picture_loadButton"
            android:layout_marginTop="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="30dp"
            android:text="@string/eduBack"/>
        <TextView
            android:id="@+id/formOfCareer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/eduBack"
            android:layout_marginTop="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="30dp"
            android:text="@string/formOfCareer"/>
        <TextView
            android:id="@+id/linguistics"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/formOfCareer"
            android:layout_marginTop="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="30dp"
            android:text="어학사항"/>
        <TextView
            android:id="@+id/selfIntroduction"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/linguistics"
            android:layout_marginTop="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="30dp"
            android:text="@string/selfIntroduction"/>
        <TextView
            android:id="@+id/licenses"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/selfIntroduction"
            android:layout_marginTop="25dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="30dp"
            android:text="수상 및 자격증"/>


