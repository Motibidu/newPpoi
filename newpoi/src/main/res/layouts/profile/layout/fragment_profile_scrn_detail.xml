<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/content"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_below="@+id/profile_screen_picture">
    <TextView
        android:id="@+id/profile_name_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/name"/>
    <TextView
        android:id="@+id/profile_name_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_e_name_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/engName"/>
    <TextView
        android:id="@+id/profile_e_name_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_ch_name_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/chName"/>
    <TextView
        android:id="@+id/profile_ch_name_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_rrn_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/rrn"/>
    <TextView
        android:id="@+id/profile_rrn_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textColor="@color/black"

        android:textSize="26sp"/>
    <TextView
        android:id="@+id/profile_age_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/age"/>
    <TextView
        android:id="@+id/profile_age_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textColor="@color/black"
        android:textSize="26sp"/>

    <TextView
        android:id="@+id/profile_SNS_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/SNS"/>
    <TextView
        android:id="@+id/profile_SNS_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_phoneNum_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/phoneNum"/>
    <TextView
        android:id="@+id/profile_phoneNum_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_number_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/num"/>
    <TextView
        android:id="@+id/profile_number_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_email__title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/email"/>
    <TextView
        android:id="@+id/profile_email_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>
    <TextView
        android:id="@+id/profile_addr_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:text="@string/addr"/>
    <TextView
        android:id="@+id/profile_addr_content"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="30dp"
        android:textSize="26sp"
        android:textColor="@color/black"/>

</LinearLayout>