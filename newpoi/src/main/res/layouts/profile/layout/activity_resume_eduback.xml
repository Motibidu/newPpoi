<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/themeColor">

    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:stickyHeader="@+id/buttonLayout">
        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/highschool"
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_border">
            <TextView
                android:id="@+id/highschool_Textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="30dp"
                android:text="고등학교"
                android:gravity="center"/>
            <LinearLayout
                android:id="@+id/highschool_layout0"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/highschool_Textview">
                <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/highschool_name_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="학교명"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/highschool_name_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/highschool_enterYM_Layout"
                android:layout_below="@+id/highschool_Textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="입학년월"
                app:suffixText="YYYY/MM"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/highschool_enterYM_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/highschool_graYM_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="졸업년월"
                app:suffixText="YYYY/MM"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/highschool_graYM_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/highschool_graCls_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="졸업구분"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1"
                app:suffixText="졸업/졸업 예정">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/highschool_graCls_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/university"
            android:layout_below="@+id/highschool"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_border"
            android:orientation="vertical">
        <TextView
            android:id="@+id/university_Textview"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/profile_picture_loadButton"
            android:layout_marginTop="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="30dp"
            android:text="대학교"
            android:gravity="center"/>
                <LinearLayout
                    android:layout_below="@id/university_Textview"
                    android:id="@+id/university_layout0"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/university_name_Layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="학교명"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/university_name_EditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/university_major_Layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="전공"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/university_major_EditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>
        <LinearLayout
            android:id="@+id/university_layout1"
            android:orientation="vertical"
            android:layout_below="@+id/university_layout0"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/university_enterYM_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="입학년월"
                app:suffixText="YYYY/MM"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/university_enterYM_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/university_graYM_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="졸업년월"
                app:suffixText="YYYY/MM"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/university_graYM_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/TextInputLayout_uLoc"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="소재지"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/TextInputEditText_uLoc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>
            <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/TextInputLayout_uSco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="학점"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/TextInputEditText_uSco"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>

            <com.google.android.material.textfield.TextInputLayout
                android:layout_below="@+id/university_layout1"
                android:id="@+id/university_graCls_Layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="졸업구분"
                app:suffixText="졸업/졸업 예정"
                app:hintTextColor="@color/themeColor"
                app:boxStrokeColor="@color/themeColor"
                android:layout_weight="1">
                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/university_graCls_EditText"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/layout_border"
                    android:singleLine="true"/>
            </com.google.android.material.textfield.TextInputLayout>

        </LinearLayout>


            </LinearLayout>
        <RelativeLayout
            android:id="@+id/master"
            android:layout_below="@+id/university"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/layout_border">
            <TextView
                android:id="@+id/master_Textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/profile_picture_loadButton"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="30dp"
                android:text="대학교(석사)"
                android:gravity="center"
                />
            <LinearLayout
                android:id="@+id/master_layout0"
                android:orientation="horizontal"
                android:layout_below="@+id/master_Textview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_name_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="학교명"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_name_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/masterMajor_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="전공"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_major_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/master_layout1"
                android:layout_below="@+id/master_layout0"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_enterYM_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="입학년월"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_enterYM_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_graYM_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="졸업년월"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_graYM_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_mLoc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="소재지"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_mLoc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_mSco"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="학점"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_mSco"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/master_layout2"
                android:layout_below="@+id/master_layout1"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_graCls_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="졸업구분"
                    app:suffixText="졸업/졸업예정"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_graCls_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_graThe_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="졸업논문"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_graThe_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/master_LAB_Layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="LAB"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/master_LAB_EditText"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
        </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/buttonLayout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/backBtn"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="18dp"
                        android:layout_marginTop="13dp"
                        android:src="@drawable/ic_baseline_arrow_back_24"
                        android:background="@drawable/btn_back"/>
                    <android.widget.Button
                        android:id="@+id/cmpltBtn"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:background="@drawable/radius"
                        android:layout_marginTop="13dp"
                        android:textColor="@color/white"
                        android:layout_alignParentRight="true"
                        android:layout_marginRight="18dp"
                        android:text="@string/complete"/>
                </RelativeLayout>
        </RelativeLayout>
        </FrameLayout>
    </com.amar.library.ui.StickyScrollView>
</RelativeLayout>