<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/themeColor"
    tools:context=".gnrtDoc.EmploymentContractActivity">
    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:stickyHeader="@+id/headerLayout"
        app:stickyFooter="@+id/RelativeLayout_btns">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <RelativeLayout
                    android:id="@+id/main_layout"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/image_main0"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true" />
                    <ImageView
                        android:id="@+id/image_main1"
                        android:layout_below="@+id/image_main0"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true" />
                    <ImageView
                        android:id="@+id/image_main2"
                        android:layout_below="@+id/image_main1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:adjustViewBounds="true" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/RelativeLayout_btns"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:background="@drawable/layout_border"
                    android:layout_below="@+id/main_layout">
                    <TextView
                        android:id="@+id/Text_title"
                        android:layout_width="wrap_content"
                        android:layout_height="40dp"
                        android:layout_marginVertical="10dp"
                        android:text="@string/title"
                        android:textSize="18sp"
                        android:textAlignment="center"
                        android:gravity="center"
                        android:layout_centerVertical="true"
                        android:textColor="#34495e" />
                    <EditText
                        android:id="@+id/editText_title"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:layout_marginVertical="10dp"
                        android:hint="@string/fileTitle"
                        android:layout_toRightOf="@id/Text_title"
                        android:textSize="18sp"
                        android:textAlignment="center"
                        android:gravity="center"
                        android:layout_centerVertical="true"
                        android:textColor="#34495e"
                        android:singleLine="true"/>
                    <android.widget.Button
                        android:id="@+id/btn_download"
                        android:layout_width="78dp"
                        android:layout_height="52dp"
                        android:layout_toRightOf="@id/editText_title"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"
                        android:text="@string/save_only_template"
                        android:textColor="@color/white"
                        android:background="@drawable/btn_maintheme" />
                    <android.widget.Button
                        android:id="@+id/btn_create"
                        android:layout_width="78dp"
                        android:layout_height="52dp"
                        android:layout_toRightOf="@+id/btn_download"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"
                        android:text="@string/create"
                        android:background="@drawable/btn_maintheme"
                        android:textColor="@color/white"/>
                    <ImageButton
                        android:id="@+id/imageBtn_more"
                        android:layout_width="34dp"
                        android:layout_height="52dp"
                        android:layout_toRightOf="@+id/btn_create"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"
                        android:backgroundTint="@color/themeColor"
                        android:src="@drawable/ic_baseline_more_vert_24" />
                </RelativeLayout>
                <LinearLayout
                    android:id="@+id/LinearLayout_bO_details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:weightSum="3"
                    android:layout_below="@+id/RelativeLayout_btns"
                    android:background="@drawable/layout_border"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="6"
                        android:background="@drawable/linearlayout_title">
                        <TextView
                            android:id="@+id/Text_bO_details"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:layout_marginStart="25dp"
                            android:layout_marginEnd="30dp"
                            android:text="@string/bO_details"
                            android:textSize="25sp"
                            android:gravity="start"
                            android:layout_weight="5"/>
                        <TextView
                            android:id="@+id/Text_progress"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="12dp"
                            android:text="1/4"
                            android:textSize="25sp"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_bON"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/bON"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_bON"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_bOA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/bOA"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_bOA"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_bOCI"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/bOCI"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_bOCI"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_bN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/bN"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_bN"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/LinearLayout_w_details"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:weightSum="3"
                    android:layout_below="@+id/LinearLayout_bO_details"
                    android:background="@drawable/layout_border"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="6">
                        <TextView
                            android:id="@+id/Text_w_details"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginStart="25dp"
                            android:layout_marginEnd="30dp"
                            android:text="@string/w_details"
                            android:textSize="25sp"
                            android:gravity="start"
                            android:layout_weight="5"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="2/4"
                            android:textSize="25sp"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_wN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/wN"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_wN"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_wA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/wA"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_wA"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                    <com.google.android.material.textfield.TextInputLayout
                        android:id="@+id/TextInputLayout_wCI"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/wCI"
                        app:hintTextColor="@color/themeColor"
                        app:boxStrokeColor="@color/themeColor"
                        android:layout_weight="1">
                        <com.google.android.material.textfield.TextInputEditText
                            android:id="@+id/TextInputEditText_wCI"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/layout_border"
                            android:singleLine="true"/>
                    </com.google.android.material.textfield.TextInputLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/LinearLayout_cC"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:weightSum="3"
                    android:layout_below="@+id/LinearLayout_w_details"
                    android:background="@drawable/layout_border"
                    >
                    <LinearLayout
                        android:id="@+id/LinearLayout_cC_title"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="6">
                        <TextView
                            android:id="@+id/Text_cC"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginStart="25dp"
                            android:layout_marginEnd="30dp"
                            android:text="@string/cC"
                            android:textSize="25sp"
                            android:gravity="start"
                            android:layout_weight="5"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="3/4"
                            android:textSize="25sp"
                            android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/CalenderLayout_wSD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:orientation="horizontal"
                        android:weightSum="8">
                        <ImageView
                            android:id="@+id/Image_wSD"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:src="@drawable/ic_baseline_calendar_today_24"/>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_wSD"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="@string/wSD"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="6">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_wSD"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAlignment="textStart"
                            android:paddingTop="10sp"
                            android:textSize="25sp"
                            android:text="@string/from"
                            android:layout_weight="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/CalenderLayout_wED"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:orientation="horizontal"
                        android:weightSum="8">
                        <ImageView
                            android:id="@+id/Image_wED"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:src="@drawable/ic_baseline_calendar_today_24"/>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_wED"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="@string/wED"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="6">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_wED"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAlignment="textStart"
                            android:paddingTop="10sp"
                            android:textSize="25sp"
                            android:text="@string/to"
                            android:layout_weight="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/TimeLayout_wST"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:orientation="horizontal"
                        android:weightSum="8">
                        <ImageView
                            android:id="@+id/Image_wST"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:src="@drawable/ic_baseline_access_time_24"/>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_wST"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="@string/wST"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="6">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_wST"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAlignment="textStart"
                            android:paddingTop="10sp"
                            android:textSize="25sp"
                            android:text="@string/from"
                            android:layout_weight="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/TimeLayout_wET"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:orientation="horizontal"
                        android:weightSum="8">
                        <ImageView
                            android:id="@+id/Image_wET"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:src="@drawable/ic_baseline_access_time_24"/>
                        <com.google.android.material.textfield.TextInputLayout
                            android:id="@+id/TextInputLayout_wET"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:hint="@string/wET"
                            app:hintTextColor="@color/themeColor"
                            app:boxStrokeColor="@color/themeColor"
                            android:layout_weight="6">
                            <com.google.android.material.textfield.TextInputEditText
                                android:id="@+id/TextInputEditText_wET"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/layout_border"
                                android:singleLine="true"/>
                        </com.google.android.material.textfield.TextInputLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:textAlignment="textStart"
                            android:paddingTop="10sp"
                            android:textSize="25sp"
                            android:text="@string/to"
                            android:layout_weight="2"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/LinearLayout_wC"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:weightSum="3"
                    android:layout_below="@+id/LinearLayout_cC"
                    android:background="@drawable/layout_border"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:weightSum="6">
                        <TextView
                            android:id="@+id/Text_wC"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:layout_marginStart="25dp"
                            android:layout_marginEnd="30dp"
                            android:text="@string/wC"
                            android:textSize="25sp"
                            android:gravity="start"
                            android:layout_weight="5"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="25dp"
                            android:text="4/4"
                            android:textSize="25sp"
                            android:layout_weight="1"/>
                    </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_pOW"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/pOW"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_pOW"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bI"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bI"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bI"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_wD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/wD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_wD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_w"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/w"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_w"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sIC"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sIC"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sIC"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

                <RelativeLayout
                    android:id="@+id/headerLayout"
                    android:layout_width="match_parent"
                    android:layout_height="60dp">
                    <ImageButton
                        android:id="@+id/backBtn"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="18dp"
                        android:layout_centerVertical="true"
                        android:src="@drawable/ic_baseline_arrow_back_24"
                        android:background="@drawable/btn_back"/>
                </RelativeLayout>
            </RelativeLayout>
    </com.amar.library.ui.StickyScrollView>
    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />
</androidx.constraintlayout.widget.ConstraintLayout>