<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/themeColor"
    tools:context=".gnrtDoc.EmploymentContractActivity">
    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:stickyHeader="@+id/headerLayout"
        app:stickyFooter="@+id/RelativeLayout_btns">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/main_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/image_main0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
                <ImageView
                    android:id="@+id/image_main1"
                    android:layout_below="@+id/image_main0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
                <ImageView
                    android:id="@+id/image_main2"
                    android:layout_below="@+id/image_main1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/RelativeLayout_btns"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/layout_border"
                android:layout_below="@+id/main_layout">
                <TextView
                    android:id="@+id/Text_title"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginVertical="10dp"
                    android:text="@string/title"
                    android:textSize="18sp"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_centerVertical="true"
                    android:textColor="#34495e" />
                <EditText
                    android:id="@+id/editText_title"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_marginVertical="10dp"
                    android:hint="@string/fileTitle"
                    android:layout_toRightOf="@id/Text_title"
                    android:textSize="18sp"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_centerVertical="true"
                    android:textColor="#34495e"
                    android:singleLine="true"/>
                <android.widget.Button
                    android:id="@+id/btn_download"
                    android:layout_width="78dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@id/editText_title"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:text="@string/save_only_template"
                    android:textColor="@color/white"
                    android:background="@drawable/btn_maintheme" />
                <android.widget.Button
                    android:id="@+id/btn_create"
                    android:layout_width="78dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@+id/btn_download"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:text="@string/create"
                    android:background="@drawable/btn_maintheme"
                    android:textColor="@color/white"/>
                <ImageButton
                    android:id="@+id/imageBtn_more"
                    android:layout_width="34dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@+id/btn_create"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:backgroundTint="@color/themeColor"
                    android:src="@drawable/ic_baseline_more_vert_24" />
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_inherited_real_estate_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/RelativeLayout_btns"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    android:background="@drawable/linearlayout_title">
                    <TextView
                        android:id="@+id/Text_inherited_real_estate_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginEnd="30dp"
                        android:text="@string/inherited_real_estate_details"
                        android:textSize="25sp"
                        android:gravity="start"
                        android:layout_weight="5"/>
                    <TextView
                        android:id="@+id/Text_progress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="1/3"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bLoc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bLoc"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bLoc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bUAA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bUAA"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bUAA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_lLoc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/lLoc"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_lLoc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_lAA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/lAA"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_lAA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_prin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/prin"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_prin"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_wON"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/wON"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_wON"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_sM_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_inherited_real_estate_details"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_title"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:id="@+id/Text_sM_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="12dp"
                        android:text="@string/sM"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="2/3"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sM1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sM1"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sM1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sM2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sM2"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sM2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sM3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sM3"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sM3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_gap_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_sM_details"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_title"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:id="@+id/Text_gap_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="12dp"
                        android:text="@string/gap_details"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="3/4"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/gapN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapAddr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/gapAddr"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapAddr"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapRRN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/gapRRN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapRRN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapPN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/gapPN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapPN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_docD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/docD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_docD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_eul_details"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_gap_details"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_title"
                    android:orientation="horizontal"

                    android:weightSum="6">
                    <TextView
                        android:id="@+id/Text_eul_details"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="12dp"
                        android:text="@string/eul_details"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="4/4"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/eulN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulAddr"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/eulAddr"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulAddr"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulRRN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/eulRRN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulRRN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulPN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/eulPN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulPN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bR1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bR1"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bR1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bR2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bR2"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bR2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/headerLayout"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageButton
                    android:id="@+id/imageBtn_back"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_baseline_arrow_back_24"
                    android:background="@drawable/btn_back"/>
            </RelativeLayout>
        </RelativeLayout>
    </com.amar.library.ui.StickyScrollView>
    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />

</androidx.constraintlayout.widget.ConstraintLayout>