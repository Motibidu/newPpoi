<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/themeColor"
    tools:context=".gnrtDoc.EmploymentContractActivity">
    <com.amar.library.ui.StickyScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:stickyHeader="@+id/headerLayout"
        app:stickyFooter="@+id/RelativeLayout_btns">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id="@+id/main_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <ImageView
                    android:id="@+id/image_main0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
                <ImageView
                    android:id="@+id/image_main1"
                    android:layout_below="@+id/image_main0"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
                <ImageView
                    android:id="@+id/image_main2"
                    android:layout_below="@+id/image_main1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:adjustViewBounds="true" />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/RelativeLayout_btns"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/layout_border"
                android:layout_below="@+id/main_layout">
                <TextView
                    android:id="@+id/Text_title"
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:layout_marginVertical="10dp"
                    android:text="@string/title"
                    android:textSize="18sp"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_centerVertical="true"
                    android:textColor="#34495e" />
                <EditText
                    android:id="@+id/editText_title"
                    android:layout_width="130dp"
                    android:layout_height="40dp"
                    android:layout_marginVertical="10dp"
                    android:hint="@string/fileTitle"
                    android:layout_toRightOf="@id/Text_title"
                    android:textSize="18sp"
                    android:textAlignment="center"
                    android:gravity="center"
                    android:layout_centerVertical="true"
                    android:textColor="#34495e"
                    android:singleLine="true"/>
                <android.widget.Button
                    android:id="@+id/btn_download"
                    android:layout_width="78dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@id/editText_title"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:text="@string/save_only_template"
                    android:textColor="@color/white"
                    android:background="@drawable/btn_maintheme" />
                <android.widget.Button
                    android:id="@+id/btn_create"
                    android:layout_width="78dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@+id/btn_download"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:text="@string/create"
                    android:background="@drawable/btn_maintheme"
                    android:textColor="@color/white"/>
                <ImageButton
                    android:id="@+id/imageBtn_more"
                    android:layout_width="34dp"
                    android:layout_height="52dp"
                    android:layout_toRightOf="@+id/btn_create"
                    android:layout_marginStart="10dp"
                    android:layout_centerVertical="true"
                    android:backgroundTint="@color/themeColor"
                    android:src="@drawable/ic_baseline_more_vert_24" />
            </RelativeLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_gapD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/RelativeLayout_btns"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    android:background="@drawable/linearlayout_title">
                    <TextView
                        android:id="@+id/Text_gapD"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_marginStart="25dp"
                        android:layout_marginEnd="30dp"
                        android:text="@string/gapD"
                        android:textSize="25sp"
                        android:gravity="start"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="1/4"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/name"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapL"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/Loc"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapL"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapPN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/phoneNum"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapPN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_gapBN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bN"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_gapBN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_eulD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_gapD"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_title"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:id="@+id/Text_eulD"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="12dp"
                        android:text="@string/eulD"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="2/4"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/name"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulRRN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/rrn"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulRRN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/addr"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_eulPN"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/phoneNum"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_eulPN"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_contractD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_eulD"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/linearlayout_title"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <TextView
                        android:id="@+id/Text_contractD"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="25dp"
                        android:layout_marginTop="12dp"
                        android:text="@string/contractD"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="3/4"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_cT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/cT"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_cT"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sCP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sCP"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sCP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_tCP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/tCP"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_tCP"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_cD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/cD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_cD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_dD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/dD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_dD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_nD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/nD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_nD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>

            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_payD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_contractD"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:id="@+id/LinearLayout_payD_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    android:background="@drawable/linearlayout_title">
                    <TextView
                        android:id="@+id/Text_payD"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_marginStart="25dp"
                        android:text="@string/payD"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="4/5"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_tCA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/tCA"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_tCA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_sA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/sA"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_sA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_bA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/bA"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_bA"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/LinearLayout_compensationDamagesD"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:weightSum="3"
                android:layout_below="@+id/LinearLayout_payD"
                android:background="@drawable/layout_border"
                >
                <LinearLayout
                    android:id="@+id/LinearLayout_compensationDamagesD_title"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6"
                    android:background="@drawable/linearlayout_title">
                    <TextView
                        android:id="@+id/Text_compensationDamagesD"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:layout_marginStart="25dp"
                        android:text="@string/compensationDamagesD"
                        android:textSize="25sp"
                        android:layout_weight="5"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="12dp"
                        android:text="5/5"
                        android:textSize="25sp"
                        android:layout_weight="1"/>
                </LinearLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_hTC"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/hTC"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_hTC"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:id="@+id/TextInputLayout_cTD"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/cTD"
                    app:hintTextColor="@color/themeColor"
                    app:boxStrokeColor="@color/themeColor"
                    android:layout_weight="1">
                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/TextInputEditText_cTD"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/layout_border"
                        android:singleLine="true"/>
                </com.google.android.material.textfield.TextInputLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/headerLayout"
                android:layout_width="match_parent"
                android:layout_height="60dp">
                <ImageButton
                    android:id="@+id/imageBtn_back"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginStart="18dp"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_baseline_arrow_back_24"
                    android:background="@drawable/btn_back"/>
            </RelativeLayout>
        </RelativeLayout>

    </com.amar.library.ui.StickyScrollView>
    <ProgressBar
        android:id="@+id/progressBar"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" />
</androidx.constraintlayout.widget.ConstraintLayout>