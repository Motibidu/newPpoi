<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.dstadler.poiandroidtest.newpoi" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:requestLegacyExternalStorage="true"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".add_screen"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".expanded_screen"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:parentActivityName=".add_screen">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".profile_screen"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".account_setting"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:parentActivityName=".profile_screen">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".profile_setting"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:parentActivityName=".profile_screen">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".add_screen_newpoi">
            <intent-filter>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DocumentListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DocumentDetailActivity"
            android:label="@string/title_document_detail"
            android:parentActivityName=".DocumentListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".DocumentListActivity" />
        </activity>
    </application>

</manifest>
